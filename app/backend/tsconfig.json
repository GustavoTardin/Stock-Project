{
    "compilerOptions": {
        "outDir": "./dist",
        "rootDir": "./",
        "target": "es2019",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "resolveJsonModule": true
    },
    "exclude": [
        "**/node_modules*"
    ],
    "include": [
        "./src/**/*.ts"
    ]
}